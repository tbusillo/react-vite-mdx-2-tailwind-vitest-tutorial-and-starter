{
  "name": "react-vite-mdx-tailwind",
  "version": "0.0.1",
  "description": "An example application using React 18, Vite, MDX 2, and Tailwind CSS v3",
  "main": "index.tsx",
  "author": "Tom Busillo",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pretty": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky install",
    "lint-staged": "yarn pretty && yarn lint",
    "pre-commit": "yarn format && lint-staged --config lint-staged.json",
    "generate": "plop",
    "test:unit": "vite-jest --no-cache",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/mdx": "^2.1.1",
    "@mdx-js/rollup": "^2.1.1",
    "@monaco-editor/react": "^4.4.4",
    "@tailwindcss/forms": "^0.5.1",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.5.0",
    "@types/mocha": "^9.1.1",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.0",
    "happy-dom": "^3.1.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mocha": "^10.0.0",
    "monaco-editor": "^0.33.0",
    "plop": "^3.1.0",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "refractor": "^4.6.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-shiki": "^0.0.9",
    "rehype-slug": "^5.0.1",
    "rehype-slug-custom-id": "^1.1.0",
    "remark-code-frontmatter": "^1.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-github": "^11.2.2",
    "remark-images": "^3.1.0",
    "remark-mdx": "^2.1.1",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
    "rollup": "^2.72.1",
    "shiki": "^0.10.1",
    "tailwindcss": "^3.0.24",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.12.0",
    "babel-jest": "^28.1.0",
    "c8": "^7.11.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-vitest": "^0.0.8",
    "husky": "^7.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-eslint": "^1.6.0",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.0"
  }
}
